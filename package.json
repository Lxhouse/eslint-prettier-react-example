{
  "name": "eslint-prettier-react-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Lxhouse/eslint-prettier-react-example.git",
  "author": "zhemu <lingx@ibuscloud.com>",
  "license": "MIT",
  "scripts": {
